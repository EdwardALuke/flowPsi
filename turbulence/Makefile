all: SA KOmega RKE DHRL TSM


SA: FRC
	$(MAKE) -C SA FLOWPSI_BASE="$(FLOWPSI_BASE)" install_local

KOmega: FRC
	$(MAKE) -C KOmega FLOWPSI_BASE="$(FLOWPSI_BASE)" install_local

RKE: FRC
	$(MAKE) -C RKE FLOWPSI_BASE="$(FLOWPSI_BASE)" install_local

DHRL: FRC
	$(MAKE) -C DHRL FLOWPSI_BASE="$(FLOWPSI_BASE)" install_local


TSM: FRC
	$(MAKE) -C TSM FLOWPSI_BASE="$(FLOWPSI_BASE)" install_local

install: FRC
	$(MAKE) -C SA -k FLOWPSI_BASE="$(FLOWPSI_BASE)" INSTALL_PATH="$(INSTALL_PATH)" install
	$(MAKE) -C KOmega -k FLOWPSI_BASE="$(FLOWPSI_BASE)" INSTALL_PATH="$(INSTALL_PATH)" install
	$(MAKE) -C RKE -k FLOWPSI_BASE="$(FLOWPSI_BASE)" INSTALL_PATH="$(INSTALL_PATH)" install
	$(MAKE) -C DHRL -k FLOWPSI_BASE="$(FLOWPSI_BASE)" INSTALL_PATH="$(INSTALL_PATH)" install
	$(MAKE) -C TSM -k FLOWPSI_BASE="$(FLOWPSI_BASE)" INSTALL_PATH="$(INSTALL_PATH)" install

clean: FRC
	$(MAKE) -C SA -k FLOWPSI_BASE="$(FLOWPSI_BASE)" clean
	$(MAKE) -C KOmega -k FLOWPSI_BASE="$(FLOWPSI_BASE)" clean
	$(MAKE) -C RKE -k FLOWPSI_BASE="$(FLOWPSI_BASE)" clean
	$(MAKE) -C DHRL -k FLOWPSI_BASE="$(FLOWPSI_BASE)" clean
	$(MAKE) -C TSM -k FLOWPSI_BASE="$(FLOWPSI_BASE)" clean

distclean: FRC
	$(MAKE) -C SA -k FLOWPSI_BASE="$(FLOWPSI_BASE)" distclean
	$(MAKE) -C KOmega -k FLOWPSI_BASE="$(FLOWPSI_BASE)" distclean
	$(MAKE) -C RKE -k FLOWPSI_BASE="$(FLOWPSI_BASE)" distclean
	$(MAKE) -C DHRL -k FLOWPSI_BASE="$(FLOWPSI_BASE)" distclean
	$(MAKE) -C TSM -k FLOWPSI_BASE="$(FLOWPSI_BASE)" distclean

FRC: 

.PHONY: all SA KOmega RKE DHRL clean distclean FRC
